    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 4040,
            "host": "127.0.0.1"
        },
        {
            "name": "Launch mw-host-agent",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}/build/mw-host-agent",
            "args": ["start", "--otel-config-file" , "${workspaceFolder}/otel-config.yaml"],
            "env": {},
            "showLog": true
        }
    ]
}